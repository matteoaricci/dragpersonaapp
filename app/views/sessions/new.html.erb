<body class="login_body">



<br><br>
<div class="login_form">
<%= form_tag '/login' do %>
    <h4 style="color: pink;"> <%= label_tag :username %>
    <%= text_field_tag :username %></h4>
   <h4 style="color: pink;"><%= label_tag :password %>
    <%= password_field_tag :password %></h4>
    
    <%= submit_tag "Login", class:"btn btn-lg button_pink" %>

<% end %>

<br>
<button class="rounded_button rounded"><%= link_to "Make a New Queen", new_user_path %></button>
   </div>
</body>