<h1>All Houses</h1>

<ul>
    <% @houses.each do |house| %>
    <li>
        <%= link_to house.name, house_path(house) %>
    </li>
    Members: <%= house.count_users %>
    <% end %>
</ul>
<br>
<button class="rounded_button_medium"><%= link_to "Make a New House", new_house_path %></button><br>
<button class="rounded_button bottom"><%= link_to "Back to Home Page", homepage_path %></button>